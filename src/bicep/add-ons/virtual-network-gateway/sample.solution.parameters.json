{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "vgwName": {
      "value": "<example: mlz-vgw-hub-va-dev>"
    },
    "vgwLocation": {
      "value": "usgovvirginia"
    },
    "vgwPublicIpAddressNames": {
      "value": [
        "<example: mlz-pip-vgw-hub-va-dev-1>",
        "<example: mlz-pip-vgw-hub-va-dev-2"
      ]
    },
    "vgwSku": {
      "value": "<see allowed values in solution.bicep> VpnGw2"
    },
    "localNetworkGatewayName": {
      "value": "<example: mlz-lng-hub-va-dev>"
    },
    "localGatewayIpAddress": {
      "value": "<publicipaddressforonpremvpntermination>"
    },
    "allowedAzureAddressPrefixes": {
      "value": [
        "10.0.130.0/24",
        "10.0.131.0/24",
        "10.0.132.0/24",
        "10.0.128.0/23"
      ]
    },
    "localAddressPrefixes": {
      "value": [
        "10.1.0.0/16"
      ]
    },
    "useSharedKey": {
      "value": true
    },
    "hubVirtualNetworkResourceId": {
      "value": "/subscriptions/<subscriptionid>/resourceGroups/<resourcegroupname>/providers/Microsoft.Network/virtualNetworks/<virtualnetworkname>"
    },
    "vnetResourceIdList": {
      "value": [
        "/subscriptions/<subscriptionid>/resourceGroups/<resourcegroupname>/providers/Microsoft.Network/virtualNetworks/<virtualnetworkname",
        "/subscriptions/<subscriptionid>/resourceGroups/<resourcegroupname>/providers/Microsoft.Network/virtualNetworks/<virtualnetworkname",
        "/subscriptions/<subscriptionid>/resourceGroups/<resourcegroupname>/providers/Microsoft.Network/virtualNetworks/<virtualnetworkname"
      ]
    },
    "azureFirewallName": {
      "value": "<example: mlz-afw-hub-va-dev>"
    },
    "vgwRouteTableName": {
      "value": "<example: mlz-vgw-rt-hub-va-dev>"
    },
    "hubVnetRouteTableName": {
      "value": "<example: mlz-rt-hub-va-dev>"
    }
  }
}